    <sql id="selectSysPersonnelVo">
        select personnel_id, personnel_type, file_id, personnel_name, personnel_nameEn, personnel_introduce, personnel_introduceEn, personnel_address, personnel_addressEn, labs, offices,tel_number, fax_number, email, twitter, linkedIn, personnel_date, current_position, current_positionEn, graduate_type, team_type,personnel_contact, personnel_contactEn, create_time, create_by, update_by, update_time, del_flag from sys_personnel
    </sql>

    <select id="selectSysPersonnelList" parameterType="SysPersonnel" resultMap="SysPersonnelResult">
        <include refid="selectSysPersonnelVo"/>
        <where>  
            <if test="personnelType != null  and personnelType != ''"> and personnel_type = #{personnelType}</if>
            <if test="fileId != null "> and file_id = #{fileId}</if>
            <if test="personnelName != null  and personnelName != ''"> and personnel_name like concat('%', #{personnelName}, '%')</if>
            <if test="personnelNameen != null  and personnelNameen != ''"> and personnel_nameEn = #{personnelNameen}</if>
            <if test="personnelIntroduce != null  and personnelIntroduce != ''"> and personnel_introduce = #{personnelIntroduce}</if>
            <if test="personnelIntroduceen != null  and personnelIntroduceen != ''"> and personnel_introduceEn = #{personnelIntroduceen}</if>
            <if test="personnelAddress != null  and personnelAddress != ''"> and personnel_address = #{personnelAddress}</if>
            <if test="personnelAddressen != null  and personnelAddressen != ''"> and personnel_addressEn = #{personnelAddressen}</if>
            <if test="labs != null  and labs != ''"> and labs = #{labs}</if>
            <if test="offices != null  and offices != ''"> and offices = #{offices}</if>
            <if test="telNumber != null  and telNumber != ''"> and tel_number = #{telNumber}</if>
            <if test="faxNumber != null  and faxNumber != ''"> and fax_number = #{faxNumber}</if>
            <if test="email != null  and email != ''"> and email = #{email}</if>
            <if test="twitter != null  and twitter != ''"> and twitter = #{twitter}</if>
            <if test="linkedin != null  and linkedin != ''"> and linkedIn = #{linkedin}</if>
            <if test="personnelDate != null  and personnelDate != ''"> and personnel_date = #{personnelDate}</if>
            <if test="currentPosition != null  and currentPosition != ''"> and current_position = #{currentPosition}</if>
            <if test="currentPositionen != null  and currentPositionen != ''"> and current_positionEn = #{currentPositionen}</if>
            <if test="graduateType != null  and graduateType != ''"> and graduate_type = #{graduateType}</if>
            <if test="teamType != null  and teamType != ''"> and team_type = #{teamType}</if>
            <if test="personnelContact != null  and personnelContact != ''"> and personnel_contact = #{personnelContact}</if>
            <if test="personnelContacten != null  and personnelContacten != ''"> and personnel_contactEn = #{personnelContacten}</if>
        </where>
    </select>
    
    <select id="selectSysPersonnelByPersonnelId" parameterType="Long" resultMap="SysPersonnelResult">
        <include refid="selectSysPersonnelVo"/>
        where personnel_id = #{personnelId}
    </select>
        
    <insert id="insertSysPersonnel" parameterType="SysPersonnel" useGeneratedKeys="true" keyProperty="personnelId">
        insert into sys_personnel
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="personnelType != null">personnel_type,</if>
            <if test="fileId != null">file_id,</if>
            <if test="personnelName != null">personnel_name,</if>
            <if test="personnelNameen != null">personnel_nameEn,</if>
            <if test="personnelIntroduce != null">personnel_introduce,</if>
            <if test="personnelIntroduceen != null">personnel_introduceEn,</if>
            <if test="personnelAddress != null">personnel_address,</if>
            <if test="personnelAddressen != null">personnel_addressEn,</if>

            <if test="labs != null">labs,</if>
            <if test="offices != null">offices,</if>

            <if test="telNumber != null">tel_number,</if>
            <if test="faxNumber != null">fax_number,</if>
            <if test="email != null">email,</if>
            <if test="twitter != null">twitter,</if>
            <if test="linkedin != null">linkedIn,</if>
            <if test="personnelDate != null">personnel_date,</if>
            <if test="currentPosition != null">current_position,</if>
            <if test="currentPositionen != null">current_positionEn,</if>
            <if test="graduateType != null">graduate_type,</if>
            <if test="teamType != null">team_type,</if>
            <if test="personnelContact != null">personnel_contact,</if>
            <if test="personnelContacten != null">personnel_contactEn,</if>
            <if test="createTime != null">create_time,</if>
            <if test="createBy != null">create_by,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="delFlag != null">del_flag,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="personnelType != null">#{personnelType},</if>
            <if test="fileId != null">#{fileId},</if>
            <if test="personnelName != null">#{personnelName},</if>
