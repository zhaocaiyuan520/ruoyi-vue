    <update id="updateSysPersonnel" parameterType="SysPersonnel">
        update sys_personnel
        <trim prefix="SET" suffixOverrides=",">
            <if test="personnelType != null">personnel_type = #{personnelType},</if>
            <if test="fileId != null">file_id = #{fileId},</if>
            <if test="personnelName != null">personnel_name = #{personnelName},</if>
            <if test="personnelNameen != null">personnel_nameEn = #{personnelNameen},</if>
            <if test="personnelIntroduce != null">personnel_introduce = #{personnelIntroduce},</if>
