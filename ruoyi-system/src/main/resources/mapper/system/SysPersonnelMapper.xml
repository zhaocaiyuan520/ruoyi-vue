            <if test="twitter != null">#{twitter},</if>
            <if test="linkedin != null">#{linkedin},</if>
            <if test="personnelDate != null">#{personnelDate},</if>
            <if test="currentPosition != null">#{currentPosition},</if>
            <if test="currentPositionen != null">#{currentPositionen},</if>
            <if test="graduateType != null">#{graduateType},</if>
            <if test="personnelContact != null">#{personnelContact},</if>
